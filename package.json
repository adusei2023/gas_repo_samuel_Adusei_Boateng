{
  "name": "gas-project",
  "version": "1.0.0",
  "description": "GitHub Actions Staging - CI/CD Pipeline with Blue-Green and Canary Deployments",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "echo 'Build successful - No compilation needed for Node.js'",
    "test": "jest --coverage --testPathIgnorePatterns=e2e --passWithNoTests",
    "test:all": "jest --coverage --passWithNoTests",
    "test:unit": "jest tests/unit --passWithNoTests",
    "test:integration": "jest tests/integration --passWithNoTests",
    "test:e2e": "jest tests/e2e --forceExit --passWithNoTests",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=e2e --passWithNoTests",
    "lint": "eslint src tests --max-warnings 0 || true",
    "lint:fix": "eslint src tests --fix || true",
    "docker:build": "docker build -t gas-app -f docker/Dockerfile .",
    "docker:run": "docker run -p 3000:3000 gas-app",
    "docker:compose": "docker-compose -f docker/docker-compose.yml up",
    "docker:compose:down": "docker-compose -f docker/docker-compose.yml down"
  },
  "keywords": [
    "ci-cd",
    "github-actions",
    "blue-green-deployment",
    "canary-deployment",
    "prometheus",
    "grafana",
    "azure"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "prom-client": "^15.1.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "applicationinsights": "^2.9.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}