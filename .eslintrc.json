{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": ["error", "never"],
    "max-len": ["error", { "code": 120 }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "global-require": "off",
    "no-use-before-define": ["error", { "functions": false, "variables": false }],
    "no-promise-executor-return": "off",
    "linebreak-style": "off"
  }
}

